{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "aurelia-framework": [
        "../node_modules/aurelia-framework"
      ],
      "@aurelia-ux/modal": ["../packages/modal/src"],
      "@aurelia-ux/positioning": ["../packages/positioning/src"],
      "@aurelia-ux/sidenav": ["../packages/sidenav/src"]
    },
    "resolveJsonModule": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "newLine": "lf"
  },
  "exclude": []
}
